{
    "pipeline": {
        "name": "web-app-pipeline",
        "roleArn": "arn:aws:iam::631204218401:role/service-role/AWSCodePipelineServiceRole-eu-west-1-aws-web-app-pipeline",
        "artifactStore": {
            "type": "S3",
            "location": "codepipeline-eu-west-1-a8ca9deab45d-410b-8909-9f4f54e1992a"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "Source",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "ThirdParty",
                            "provider": "GitHub",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "Branch": "main",
                            "OAuthToken": "****",
                            "Owner": "ramsjenu",
                            "PollForSourceChanges": "false",
                            "Repo": "aws-web-app"
                        },
                        "outputArtifacts": [
                            {
                                "name": "SourceArtifact"
                            }
                        ],
                        "inputArtifacts": [],
                        "region": "eu-west-1",
                        "namespace": "SourceVariables"
                    }
                ],
                "onFailure": {
                    "result": "RETRY"
                }
            },
            {
                "name": "Build",
                "actions": [
                    {
                        "name": "Build",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ProjectName": "aws-web-app"
                        },
                        "outputArtifacts": [
                            {
                                "name": "BuildArtifact"
                            }
                        ],
                        "inputArtifacts": [
                            {
                                "name": "SourceArtifact"
                            }
                        ],
                        "region": "eu-west-1",
                        "namespace": "BuildVariables"
                    }
                ],
                "onFailure": {
                    "result": "RETRY"
                }
            },
            {
                "name": "Deploy",
                "actions": [
                    {
                        "name": "AppDeploy",
                        "actionTypeId": {
                            "category": "Deploy",
                            "owner": "AWS",
                            "provider": "CodeDeployToECS",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "AppSpecTemplateArtifact": "SourceArtifact",
                            "AppSpecTemplatePath": "appspec.yaml",
                            "ApplicationName": "webapp-bg",
                            "DeploymentGroupName": "dg-webapp-bg",
                            "Image1ArtifactName": "BuildArtifact",
                            "Image1ContainerName": "web-app-container",
                            "TaskDefinitionTemplateArtifact": "SourceArtifact",
                            "TaskDefinitionTemplatePath": "taskdef.json"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": [
                            {
                                "name": "SourceArtifact"
                            },
                            {
                                "name": "BuildArtifact"
                            }
                        ],
                        "region": "eu-west-1"
                    }
                ]
            }
        ],
        "version": 17,
        "executionMode": "QUEUED",
        "pipelineType": "V2"
    }
}
